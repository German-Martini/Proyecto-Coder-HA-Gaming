        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script src="/medios-plazavip/swift/v1/claroshop/landings/slick/slick.min.js"></script>
        <script src="/c/js/crearTarjetaProducto-SliderV2.js"></script>
        <script type="text/javascript">
            let banderaRec = false;

            consulta('https://www.googleapis.com/drive/v3/files/1eIgVMws0obonc8rpETLjAgccpEoiVCZW?alt=media&key=AIzaSyCQF6nsi7sYNSb1_5WZ3MzohMLylG0jNB0');

            
            function consulta(url_sheet) {
                let skusMasBuscadas = [];
                let contador = 0;
                let contadorProductos = 0;
                let skusTemp = [];
                let timeOut = 0;
                let contadorGral = 0;

                fetch(url_sheet)
                .then(response => {
                     if (response.status == 200) {
                        return response.json()
                    }
                }).then(result =>{
                    const feedData = result;
                    feedData.map( respuesta => {

                        if (respuesta.repetidos == false && skusMasBuscadas.length <= 56) {
                            if (contadorProductos < 18) {
                                skusTemp.push(respuesta.id);
                            }
                            if (respuesta.id) {
                                contadorProductos += 1;
                            }
                        }
                        if (contadorProductos == 18 || contadorGral == feedData.length - 1) {
                            skusMasBuscadas.push(skusTemp);
                            skusTemp = [];
                            contadorProductos = 0;
                        }
                        contadorGral += 1;
                    })
                }).then(res  => {
                    for (var i = 0; i < skusMasBuscadas.length; i++) {
                        let nombreContenedor = "contenedor" + i.toString();
                        crearTarjetaProductoDescArriba(skusMasBuscadas[i].filter(el => el != undefined).filter(el => el != ''), nombreContenedor, 18, 18, 0, 0);
                        
                    }
                }).catch(function(error) {
                    console.log('Hubo un problema con la petición Fetch:' + error.message);
                    if (!banderaRec) {
                        banderaRec = true;
                        let fecha = new Date();
                        let url_json = '/c/json/XtremePCGamingCS.json?ver=' + String(fecha.getFullYear()) + String(fecha.getMonth()) + String(fecha.getDate());
                        consulta(url_json);
                    }
                });
            }
        </script>
